{
  "expo": {
    "name": "ChatExpo",
    "description": "Simplify conversations with ChatExpo",
    "slug": "ChatExpo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/chat-expo-app-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/chat-expo-app-splash-screen.png",
      "resizeMode": "cover",
      "backgroundColor": "#f4f2f2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/chat-expo-app-favicon.png"
    }
  }
}
